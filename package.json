{
  "name": "omnee-protocol",
  "version": "1.0.0",
  "type": "module",
  "description": "oMNEE Protocol: Universal Ledger for AI Agents & RWA Tokenization - MNEE Hackathon Entry",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:auth": "hardhat test test/Authorization.test.js",
    "test:manual": "hardhat run scripts/test-auth.js --network localhost",
    "node": "hardhat node",
    "deploy": "npx hardhat run scripts/deploy.js"
  },
  "keywords": [
    "mnee",
    "stablecoin",
    "defi",
    "ai-agents",
    "rwa",
    "tokenization",
    "cross-chain",
    "ethereum",
    "hackathon",
    "programmable-money"
  ],
  "author": "krewdev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/krewdev/oMNEE-protocol.git"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "@openzeppelin/contracts-upgradeable": "^5.4.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.2",
    "@nomicfoundation/hardhat-ignition": "^0.15.16",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.17",
    "@nomicfoundation/hardhat-network-helpers": "^1.1.2",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.1.3",
    "@nomicfoundation/ignition-core": "^0.15.15",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.19.1",
    "chai": "^6.2.1",
    "concurrently": "^9.2.1",
    "ethers": "^6.16.0",
    "hardhat": "^2.27.1",
    "hardhat-gas-reporter": "^1.0.10",
    "solc": "^0.8.20",
    "solidity-coverage": "^0.8.16",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "~5.8.0"
  }
}